{
  "name": "@tibco-cloud/cli-core",
  "description": "Core Library for TIBCO cloud cli",
  "version": "0.1.0",
  "author": "TIBCO Cloud",
  "bugs": "https://github.com/tibco/cic-cli-core/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.18",
    "@types/cli-progress": "^3.9.2",
    "@types/fs-extra": "^9.0.12",
    "@types/luxon": "^1.27.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^15.0.2",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/tibco/cic-cli-core",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cli-core",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/tibco/cic-cli-core",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "watch": "tsc -w --noEmitOnError"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "cli-progress": "^3.9.0",
    "cli-table": "^0.3.6",
    "debug": "^4.3.1",
    "enquirer": "^2.3.6",
    "fs-extra": "^10.0.0",
    "keytar": "^7.7.0",
    "luxon": "^1.27.0",
    "open": "^8.2.1",
    "ora": "^5.4.1",
    "tslib": "^1.14.1"
  }
}
